package ddb.smartcenter.gateway.out;

import java.util.Date;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;

@ApiModel(value = "用户响应数据")
public class GateWayUsers {

	@ApiModelProperty(value = "用户ID", required = true)
	private int uId;
	@ApiModelProperty(value = "用户手机号", required = true)
	private int phone;
	@ApiModelProperty(value = "用户角色", required = true)
	private String roleCode;
	@ApiModelProperty(value = "绑定时间", required = true)
	private Date bindDate;
	@ApiModelProperty(value = "绑定时终端名称", required = true)
	private String deviceName;
	@ApiModelProperty(value = "绑定时终端Mac", required = true)
	private String Mac;
	
	public int getuId() {
		return uId;
	}
	public void setuId(int uId) {
		this.uId = uId;
	}
	public int getPhone() {
		return phone;
	}
	public void setPhone(int phone) {
		this.phone = phone;
	}
	public String getRoleCode() {
		return roleCode;
	}
	public void setRoleCode(String roleCode) {
		this.roleCode = roleCode;
	}
	public Date getBindDate() {
		return bindDate;
	}
	public void setBindDate(Date bindDate) {
		this.bindDate = bindDate;
	}
	public String getDeviceName() {
		return deviceName;
	}
	public void setDeviceName(String deviceName) {
		this.deviceName = deviceName;
	}
	public String getMac() {
		return Mac;
	}
	public void setMac(String mac) {
		Mac = mac;
	}
}
